<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<%= @user.name %>, <%= @user.email %>
<p>Name: <%= @user.name %> </p>
<p>Email: <%= @user.email %> </p>
<p>Created At: <%= @user.created_at %> </p>
<p>Updated At: <%= @user.updated_at %> </p>
<p>Time: <%= Time.now %> </p>